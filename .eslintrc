{
  "extends": [ "eslint:recommended" ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
  },
  "env": {
    "browser": false,
    "node": true,
    "es6": true,
  },
  "plugins": [],
  "rules": {
    "no-console": 1,
    "quotes": [1, "single"],
    "semi": [1, "always"],
    "no-case-declarations": 0,
    "no-unused-vars": 1,
  }
}
